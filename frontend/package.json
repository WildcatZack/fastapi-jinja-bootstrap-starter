{
  "name": "fastapi-jinja-bootstrap-starter-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:css": "sass src/styles.scss ../app/static/app.css",
    "build:js": "esbuild src/index.js --bundle --outfile=../app/static/app.js",
    "build": "npm run build:css && npm run build:js",
    "dev:css": "sass --watch src/styles.scss ../app/static/app.css",
    "dev:js": "esbuild src/index.js --bundle --outfile=../app/static/app.js --watch",
    "dev": "npm run dev:css & npm run dev:js"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3"
  },
  "devDependencies": {
    "esbuild": "^0.21.5",
    "sass": "^1.77.6"
  }
}
